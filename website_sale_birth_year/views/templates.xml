<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="address_inherit_birth_year" inherit_id="website_sale.address">
        <xpath expr="//select[@id='country_id']//.." position="after">

            <div class="form-group col-lg-6">
                <label class="col-form-label" for="year_of_birth">Year of Birth</label>
                <input name="year_of_birth" class="form-control" type="number" />
            </div>

        </xpath>
    </template>
</odoo>
