<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="website_sale_coupon_log_tree" model="ir.ui.view" >
        <field name="name">Website coupon tree</field>
        <field name="model">website.sale.coupon.log</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <tree string="Website coupons log">
                <field name="coupon_id"/>
                <field name="create_date" string="Used on"/>
                <field name="sale_order_id"/>
                <field name="sale_order_line_id"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

</odoo>