<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="checkout" name="Checkout quotation" inherit_id="website_sale.checkout">
        <xpath expr="//a[@href='/shop/cart']" position="replace">
            <a href="/shop/cart" class="btn btn-default mb32">
                <span class="fa fa-long-arrow-left"/> Return to Quotation</a>
        </xpath>
    </template>
</odoo>
